<div class="row">
   <div class="col-md-12">
      <div class="panel panel-default">
         <div class="panel-heading">
            <h3 class="panel-title">Listing Extra Fields</h3>
         </div>
         <div class="panel-body">

<p id="notice"><%= notice %></p>

 

<table>
  <thead>
    <tr>
      <th>Cms page</th>
      <th>Field name</th>
      <th>Field value</th>
      <th>Field setting</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extra_fields.each do |extra_field| %>
      <tr>
        <td><%= extra_field.cms_page.title %></td>
        <td><%= extra_field.field_name %></td>
        <td><%= extra_field.field_value %></td>
        <td><%= extra_field.field_setting %></td>
        <td><%= link_to 'Show', extra_field %></td>
        <td><%= link_to 'Edit', edit_extra_field_path(extra_field) %></td>
        <td><%= link_to 'Destroy', extra_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Extra field', new_extra_field_path %>
</div>
</div>

</div>
</div>



