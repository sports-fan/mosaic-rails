
        <div class="row">
                            <div class="col-md-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h3 class="panel-title"><%= t(:LISTING_PLAYED_SURVEYS) %></h3>
                                    </div>
                                    <div class="panel-body">
                                        <div class="row">
                                          <p id="notice"><%= notice %></p>

                                            <div class="col-md-12 col-sm-12 col-xs-12">
                                                <div class="table-responsive">
                                                    <table class="table">
                                                    <thead>
                                                    <tr>
                                                      <th><%= t(:USER) %></th>
                                                      <th><%= t(:Section) %></th>
                                                      <th><%= t(:Option) %></th>
                                                      <th colspan="3"></th>
                                                    </tr>
                                                  </thead>

                                                  <tbody>
                                                    <% @played_surveys.each do |played_survey| %>
                                                      <tr>
                                                        <td><%= played_survey.user_id %></td>
                                                        <td><%= played_survey.section_id %></td>
                                                        <td><%= played_survey.option %></td>
                                                        <td><%= link_to t(:show), played_survey %></td>
                                                        <td><%= link_to t(:edit), edit_played_survey_path(played_survey) %></td>
                                                        <td><%= link_to t(:destroy), played_survey, method: :delete, data: { confirm: t(:ARE_YOU_SURE) } %></td>
                                                      </tr>
                                                    <% end %>
                                                  </tbody>
                                                </table>
                                                </div>
                                            </div>
<%= link_to t(:NEW_PLAYED_SURVEY), new_played_survey_path %>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div> <!-- End row -->
