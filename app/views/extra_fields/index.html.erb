<div class="row">
   <div class="col-md-12">
      <div class="panel panel-default">
         <div class="panel-heading">
            <h3 class="panel-title"><%= t(:LISTING_EXTRA_FIELDS) %></h3>
         </div>
         <div class="panel-body">

<p id="notice"><%= notice %></p>

 

<table>
  <thead>
    <tr>
      <th><%= t(:CMS_PAGE) %></th>
      <th><%= t(:FIELD_NAME) %></th>
      <th><%= t(:FIELD_VALUE) %></th>
      <th><%= t(:FIELD_SETTING) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extra_fields.each do |extra_field| %>
      <tr>
        <td><%= extra_field.cms_page.title %></td>
        <td><%= extra_field.field_name %></td>
        <td><%= extra_field.field_value %></td>
        <td><%= extra_field.field_setting %></td>
        <td><%= link_to t(:show), extra_field %></td>
        <td><%= link_to t(:edit), edit_extra_field_path(extra_field) %></td>
        <td><%= link_to t(:destroy), extra_field, method: :delete, data: { confirm: t(:ARE_YOU_SURE) } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to t(:NEW_EXTRA_FIELD), new_extra_field_path %>
</div>
</div>

</div>
</div>



