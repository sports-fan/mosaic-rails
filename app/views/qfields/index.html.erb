<p id="notice"><%= notice %></p>

<h1>Listing Qfields</h1>

<table>
  <thead>
    <tr>
      <th>Fval</th>
      <th>Type</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qfields.each do |qfield| %>
      <tr>
        <td><%= qfield.fval %></td>
        <td><%= qfield.type_id %></td>
        <td><%= qfield.section_id %></td>
        <td><%= link_to 'Show', qfield %></td>
        <td><%= link_to 'Edit', edit_qfield_path(qfield) %></td>
        <td><%= link_to 'Destroy', qfield, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qfield', new_qfield_path %>
