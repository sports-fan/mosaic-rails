<p id="notice"><%= notice %></p>
<% if @user_variable.errors.any? %>
  <% @user_variable.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
<% end %>
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title trans-none"><%= t(:ADD_USER_VARIABLE) %>
        </h3>
      </div>
      <%= render 'form', url: ext_user_variables_path(user_id: params[:user_id]), variable: variable_names_collection.count ? Variable.find(variable_names_collection[0][1]) : {} %>
    </div>
  </div>
</div>
<!-- End row -->
<%= link_to t(:Back), admin_editUser_url(@user_variable.user) %>
