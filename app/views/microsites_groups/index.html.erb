<p id="notice"><%= notice %></p>

<h1>Listing Microsites Groups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @microsites_groups.each do |microsites_group| %>
      <tr>
        <td><%= link_to 'Show', microsites_group %></td>
        <td><%= link_to 'Edit', edit_microsites_group_path(microsites_group) %></td>
        <td><%= link_to 'Destroy', microsites_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Microsites group', new_microsites_group_path %>
